{
  "name": "crsh_backend",
  "version": "1.0.0",
  "description": "Crsh Game backend with Socket.io on Express written with TypeScript",
  "repository": "https://github.com/DylanBrody/Crash-Backend.git",
  "main": "server.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "ts-node-dev --respawn --poll --transpileOnly --no-notify ./src/server.ts",
    "build": "tsc",
    "prod": "NODE_ENV=production node ./dist/server.js"
  },
  "keywords": [
    "Socket"
  ],
  "author": "Dylan Brody",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.8",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "isaac": "^0.0.5",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "mongodb": "^6.5.0",
    "precision-timeout-interval": "^4.3.0",
    "socket.io": "^4.4.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.6",
    "@types/crypto-js": "^3.1.44",
    "@types/express": "^4.17.6",
    "@types/isaac": "^0.0.3",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/jwt-decode": "^3.1.0"
  }
